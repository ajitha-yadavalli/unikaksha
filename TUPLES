#Add an item

a=(4,6,2,8,3,1)
b=(9,)
c=a+b
print(a)
print(c)
c=list(c)
c=c[:5]+[15,20,25,4,6,2,8,3]
d=list(c)
d.append(30)
c=tuple(c)
d=tuple(d)
print(c)
print(d)


#Length of a tuple

a=input()
b=tuple(a)
print(b)
print(len(b))


#Reverse a Tuple

a=input()
a=a[::-1]
b=tuple(a)
print(b)


#Repeated element in a tuple

a=(2,4,5,6,2,3,4,4,7)
print(a)
print(a.count(5))
